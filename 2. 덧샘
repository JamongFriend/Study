import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    System.out.println("정수 A값을 입력하시오");
    int a = sc.nextInt();
    System.out.println("정수 B값을 입력하시오");
    int b = sc.nextInt();
    int sum = a + b;
    System.out.println("정수 A, B의 합: " + sum);
  }
}

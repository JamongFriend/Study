import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);
    System.out.println("정수 A값을 입력하시오");
    int a = sc.nextInt();
    System.out.println("정수 B값을 입력하시오");
    int b = sc.nextInt();
    
    System.out.println("연산자를 선택하세요(+,-,*,/,%)");
    String calc = sc.next();
    
    if(calc.equals("+")){
       System.out.println(a+b);
    }
    else if(calc.equals("-")){
      System.out.println(a-b);
    }
    else if(calc.equals("*")){
      System.out.println(a*b);
    }
    else if(calc.equals("/")){
      System.out.println(a/b);
    }
    else if(calc.equals("%")){
      System.out.println(a%b);
    }
    else
      System.out.println("잘못된 연산자입니다");
}

import java.util.Scanner;

class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in); //Scanner 객체를 생성
    
    //a,b 값을 사용자로부터 입력받는다
    System.out.println("정수 A값을 입력하시오");
    int a = sc.nextInt();
    System.out.println("정수 B값을 입력하시오");
    int b = sc.nextInt();
    
    //연산자를 사용자로부터 입력받는다
    System.out.println("연산자를 선택하세요(+,-,*,/,%)");
    String calc = sc.next(); //문자열로 입력을 받는다
    
    //입력받은 연산자를 if문을 이용하여 구별하여 값을 출력
    if(calc.equals("+"))
       System.out.println(a+b);
    else if(calc.equals("-"))
      System.out.println(a-b);
    else if(calc.equals("*"))
      System.out.println(a*b);
    else if(calc.equals("/"))
      System.out.println(a/b);
    else if(calc.equals("%"))
      System.out.println(a%b);
    else
      System.out.println("잘못된 연산자입니다");
}

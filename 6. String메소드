class Main {
  public static void main(String[] args) {
    String a = "Hello";
    String b = "Hello";
    String c = "Java";
    String d = "Hello Java";

    //String 내장 메소드인 equals로 a,b,c의 문자열 비교
    System.out.println(a.equals(b));
    System.out.println(a.equals(c));

    //String 메소드인 indexOf로 문자열d에서 "Java"가 시작되는 위치를 출력
    System.out.println(d.indexOf("Java"));
  }
}
